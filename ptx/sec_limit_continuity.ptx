    <exercise>
      <statement>
        <sidebyside>
          <img src="figures/fig01_04_ex_06"/>

          <ol cols="2">
            <li>
              <p>
                <m>\ds \lim_{x\to 1^-} f(x)</m>
              </p>
            </li>

            <li>
              <p>
                <m>\ds \lim_{x\to 1^+} f(x)</m>
              </p>
            </li>

            <li>
              <p>
                <m>\ds \lim_{x\to 1} f(x)</m>
              </p>
            </li>

            <li>
              <p>
                <m>f(1)</m>
              </p>
            </li>

            <li>
              <p>
                <m>\ds \lim_{x\to 2^-} f(x)</m>
              </p>
            </li>

